{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demo-adf-prem"
		},
		"ls_source_population_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_population'"
		},
		"ls_target_population_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_target_population'"
		},
		"ls_target_population_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdatalakeprem.dfs.core.windows.net"
		},
		"RunJob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d0e9b79-51f6-4dba-a862-c4549da42f4f/resourceGroups/DataFactory/providers/Microsoft.Storage/storageAccounts/adfprem"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demo-adf-prem_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_source_population_connectionString": {
						"value": "[parameters('ls_source_population_connectionString')]"
					},
					"ls_target_population_accountKey": {
						"value": "[parameters('ls_target_population_accountKey')]"
					},
					"ls_target_population_properties_typeProperties_url": {
						"value": "[parameters('ls_target_population_properties_typeProperties_url')]"
					},
					"RunJob_properties_typeProperties_scope": {
						"value": "[parameters('RunJob_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}